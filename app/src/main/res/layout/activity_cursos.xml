<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.Cursos">
    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/fragment_listado_curso"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@id/fragment_navigationg_bar"
        android:name="com.castraining.castraining.fragment.listadoCursos"/>
    <ImageButton
        android:layout_width="56dp"
        android:layout_height="56dp"
        style="@style/btnAppMensaje"
        android:src="@drawable/ic_baseline_insert_comment_24"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="16dp"
        app:layout_constraintBottom_toTopOf="@id/fragment_navigationg_bar"
        android:layout_marginBottom="16dp"
        app:circularflow_defaultRadius="16dp"/>
    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/fragment_navigationg_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:name="com.castraining.castraining.fragment.navigationBar"/>
</androidx.constraintlayout.widget.ConstraintLayout>